{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true,
        "jest": true,
        "browser": true
    },
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "airbnb", "prettier"],
    "rules": {
        "prettier/prettier": "error"
    }
}
